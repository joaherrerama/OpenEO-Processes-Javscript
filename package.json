{
  "name": "openeojs",
  "version": "0.0.1",
  "description": "This repository contains the based code of implementation for processes specifications. Each process is coded and specified in modules with its test and documentation code.",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "@openeo/js-commons": "^1.4.1",
    "@openeo/js-processgraphs": "^1.3.0",
    "@seald-io/nedb": "^3.0.0",
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5",
    "eslint-pre-commit-hook": "^2.0.7",
    "fse": "^4.0.1",
    "geotiff": "^2.0.5",
    "proj4": "^2.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/jest": "^28.1.4",
    "babel-jest": "^28.1.2",
    "core-js": "^3.23.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-fetch": "^3.2.10",
    "npm": "^8.13.2",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "regenerator-runtime": "^0.13.9"
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "http-server",
    "compile": "rm -rf dist/lib && tsc --build tsconfig.es5.json",
    "prepare": "husky install",
    "prepare_npm": "npm run build",
    "build": "babel src --out-dir lib",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "env": {
    "browser": true,
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaherrerama/OpenEO-processes-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaherrerama/OpenEO-processes-js/issues"
  },
  "homepage": "https://github.com/joaherrerama/OpenEO-processes-js#readme"
}
