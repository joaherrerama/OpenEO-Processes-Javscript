{
  "version": "1.0.0",
  "name": "@joaherrerama/openeojs",
  "description": "This repository contains the based code of implementation for processes specifications. Each process is coded and specified in modules with its test and documentation code.",
  "author": {
    "name": "Jorge Herrera",
    "email": "jorge@uni-muenster.de"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/joaherrerama"
  },
  "main": "lib/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "@openeo/js-commons": "^1.4.1",
    "@openeo/js-processgraphs": "^1.3.0",
    "@seald-io/nedb": "^3.0.0",
    "ajv": "^8.11.0",
    "babel-runtime": "^6.26.0",
    "cross-fetch": "^3.1.5",
    "eslint-pre-commit-hook": "^2.0.7",
    "fast-max": "^0.4.0",
    "fast-min": "^0.3.0",
    "fs-extra": "^10.1.0",
    "fse": "^4.0.1",
    "geotiff": "^2.0.4",
    "proj4": "^2.8.0",
    "require-esm-as-empty-object": "0.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "core-js": "^3.23.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "git-format-staged": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "node-fetch": "^2.6.7",
    "npm": "^8.13.2",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3"
  },
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "prepare_npm": "npm run build",
    "build": "babel src --out-dir lib",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "env": {
    "browser": true,
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaherrerama/OpenEO-processes-js.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaherrerama/OpenEO-processes-js/issues"
  },
  "nodemonConfig": {
    "watch": [
      "src/"
    ]
  },
  "homepage": "https://github.com/joaherrerama/OpenEO-processes-js#readme"
}
